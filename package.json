{
  "name": "user-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run server:dev\" \"npm run client\"",
    "start:prod": "npm run server",
    "server": "node bin/www",
    "server:dev": "nodemon bin/www --ignore frontend/ --ignore config/ --ignore routes/api/attachments/ --ignore routes/api/gcpCredential",
    "client": "npm start --prefix frontend"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.1.3",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^7.2.2",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22"
  }
}
